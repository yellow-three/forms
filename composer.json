{
    "name": "yellow-three/voyager-forms",
    "description": "Ahoy! - A package to implement forms into Voyager",
    "keywords": [
        "laravel",
        "frontend",
        "voyager",
        "forms"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Pivotal Agency",
            "homepage": "http://pivotalagency.com.au",
            "email": "tech@pivotalagency.com.au"
        }
    ],
    "require": {
        "tcg/voyager": "^1.5",
        "guzzlehttp/guzzle": "~6.0|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "classmap": [],
        "psr-4": {
            "YellowThree\\VoyagerForms\\": "src/",
            "YellowThree\\VoyagerForms\\Database\\": "database/"
        }
    },
    "autoload-dev": {
        "classmap": [],
        "psr-4": {
            "YellowThree\\VoyagerForms\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "YellowThree\\VoyagerForms\\Providers\\VoyagerFormsServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit --colors=always"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/igorw/monolog"
        }
    ]
}
